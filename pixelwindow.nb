Cell[BoxData[
 RowBox[{
  RowBox[{"W2", "[", 
   RowBox[{"k_", ",", " ", "R_"}], "]"}], "=", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"\[Pi]", " ", 
      SuperscriptBox["R", "2"]}], ")"}], 
    RowBox[{"-", "1"}]], 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"r", "  ", 
      RowBox[{"Exp", "[", 
       RowBox[{"I", " ", "k", " ", "r", " ", 
        RowBox[{"Cos", "[", "\[Phi]", "]"}]}], "]"}]}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "R"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"\[Phi]", ",", "0", ",", 
       RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"k", ">", "0"}], ",", " ", 
        RowBox[{"R", ">", "0"}]}], "}"}]}]}], "]"}]}]}]], "Input",
 CellChangeTimes->{{3.6674127499291277`*^9, 3.667412914682583*^9}, {
   3.6674644713520823`*^9, 3.667464568874733*^9}, {3.66746462309125*^9, 
