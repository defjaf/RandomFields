Cell[BoxData[
 RowBox[{
  RowBox[{"Wazim", "[", 
   RowBox[{"k_", ",", " ", "L_"}], "]"}], "=", 
  RowBox[{
   RowBox[{"Integrate", "[", " ", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{"\[ImaginaryI]", " ", "k", " ", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"]}]], " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"ArcTan", "[", 
         RowBox[{"x", ",", "y"}], "]"}], "]"}]}], "]"}], ",", " ", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], "\[Element]", 
      RowBox[{"Rectangle", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "L"}], "/", "2"}], ",", 
          RowBox[{
           RowBox[{"-", "L"}], "/", "2"}]}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"L", "/", "2"}], ",", 
          RowBox[{"L", "/", "2"}]}], "}"}]}], "]"}]}], ",", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"k", "\[GreaterEqual]", "0"}], ",", 
        RowBox[{"L", ">", "0"}]}], "}"}]}]}], "]"}], "/", 
   SuperscriptBox["L", "2"]}]}]], "Input",
